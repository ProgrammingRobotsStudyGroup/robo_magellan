<launch>
  <node name="wheel_encoder" pkg="robo_magellan" type="arduino_receiver">
    <param name="~publish_touch" value="True" />
    <param name="~touch_sensor_count" value="2" />         <!-- Number of touch sensors in use -->
    <param name="~wheel_diameter" value="0.200" />
    <param name="~ticks_per_revolution" value="80" />
    <!-- param name="~serial_device" value="/dev/ttyACM0" / -->
    <!-- /dev/astar defined in udev rules -->
    <param name="~serial_device" value="/dev/astar" />
    <param name="~serial_rate" value="115200" />
  </node>
</launch>
